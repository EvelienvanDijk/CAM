<prescribed_ozone_type>'CYCLICAL'</prescribed_ozone_type>
<prescribed_ozone_cycle_yr>1850</prescribed_ozone_cycle_yr>

<!-- Prescribed oxidants for aerosol chemistry -->
<tracer_cnst_cycle_yr>1850</tracer_cnst_cycle_yr>
